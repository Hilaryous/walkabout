<div id="map" style='width: 100%; height: 75vh;'></div>
<%= javascript_include_tag('map_style.js') %>

<script>
  $(document).ready(function initialize(){
    handler = Gmaps.build('Google');
    handler.buildMap({
      provider: {
        zoom: 8,
        center: new google.maps.LatLng(39.730859, -104.981795),
        styles: mapStyle
      },
      internal: {id: 'map'}
      }
    );
  });
</script>
